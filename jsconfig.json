{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "baseUrl": ".",
        "paths": {
            "#root/*": [
                "./*"
            ],
            "#src/*": [
                "./src/*"
            ],
            "#buttons/*": [
                "./src/buttons/*"
            ],
            "#commands/*": [
                "./src/commands/*"
            ],
            "#events/*": [
                "./src/events/*"
            ],
            "#modals/*": [
                "./src/modals/*"
            ],
            "#modules/*": [
                "./src/modules/*"
            ],
            "#stored/*": [
                "./src/stored/*"
            ],
        }
    }
}
